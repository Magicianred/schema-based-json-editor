{
  "name": "schema-based-json-editor",
  "version": "3.7.4",
  "description": "A reactjs, angular and vuejs component of schema based json editor.",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc && tsc -p demo && webpack --config demo/webpack.config.react.js && webpack --config demo/webpack.config.angular.js && webpack --config demo/webpack.config.vue.js && npm run rev",
    "tsc": "npm run clean && tsc -p src",
    "lint": "tslint \"src/**/*.tsx\" \"demo/**/*.tsx\" \"src/**/*.ts\" \"demo/**/*.ts\" \"spec/**/*.ts\"",
    "static": "mkdirp ./demo/css && cpy ./node_modules/bootstrap/dist/css/bootstrap.min.css ./demo/css/ && cp -r ./node_modules/font-awesome/css/font-awesome.min.css ./demo/css/ && cp -r ./node_modules/dragula/dist/dragula.min.css ./demo/css/ && cp -r ./node_modules/font-awesome/fonts/. ./demo/fonts && mkdir ./demo/css/highlight.js && cp -r ./node_modules/highlight.js/styles/default.css ./demo/css/highlight.js/",
    "test": "tsc -p spec && jasmine",
    "rev": "npm run clean-rev && rev-static --config demo/rev-static.config.js",
    "clean-rev": "rimraf demo/react*-*.js demo/vue*-*.js demo/angular*-*.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/schema-based-json-editor.git"
  },
  "keywords": [
    "json",
    "schema",
    "vue",
    "react",
    "angular"
  ],
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/schema-based-json-editor/issues"
  },
  "homepage": "https://github.com/plantain-00/schema-based-json-editor#readme",
  "dependencies": {
    "@types/dragula": "2.1.30",
    "@types/highlight.js": "9.1.9",
    "@types/markdown-it": "0.0.2",
    "lodash.isinteger": "4",
    "lodash.isobject": "3",
    "lodash.tointeger": "4",
    "lodash.tonumber": "4",
    "tslib": "1"
  },
  "devDependencies": {
    "@angular/common": "4.1.0",
    "@angular/compiler": "4.1.0",
    "@angular/core": "4.1.0",
    "@angular/forms": "4.1.0",
    "@angular/http": "4.1.0",
    "@angular/platform-browser": "4.1.0",
    "@angular/platform-browser-dynamic": "4.1.0",
    "@angular/router": "4.1.0",
    "@angular/upgrade": "4.1.0",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.14",
    "@types/react": "15.0.23",
    "@types/react-dom": "15.5.0",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "cpy-cli": "1.0.1",
    "dragula": "3.7.2",
    "font-awesome": "4.7.0",
    "highlight.js": "9.11.0",
    "jasmine": "2.6.0",
    "markdown-it": "8.3.1",
    "mkdirp": "0.5.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rev-static": "2.1.2",
    "rimraf": "2.6.1",
    "rxjs": "5.3.0",
    "tslint": "5.1.0",
    "typescript": "2.3.2",
    "vue": "2.3.0",
    "vue-class-component": "5.0.1",
    "webpack": "2.4.1",
    "zone.js": "0.8.9"
  }
}
